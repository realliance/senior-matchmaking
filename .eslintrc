{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "eol-last":2,
    "no-trailing-spaces":2,
    "no-tabs":2,
    "func-style":["error", "expression"]

  },
  "env": {
    "jest": true
  }
}
